<Window x:Class="Taken15.Views.Congratulations"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Taken15.Views"
        mc:Ignorable="d"
        Title="Congratulations" Height="400" Width="450" Background="#FF5B5B5B" ResizeMode="CanMinimize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="40"></RowDefinition>
        </Grid.RowDefinitions>
        <Image Source="../Img/img.jpg"></Image>
        <Button x:Name="button" Content="Thanks" HorizontalAlignment="Center"  Grid.Row="1" VerticalAlignment="Center" Width="75" Click="button_Click"/>
    </Grid>
</Window>
